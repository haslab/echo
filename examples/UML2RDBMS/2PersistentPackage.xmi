<?xml version="1.0" encoding="ASCII"?>
<uml:Package
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:uml="UML"
    xsi:schemaLocation="UML UML.ecore#UML"
    name="Company">
  <classes name="Person"
      persistent="true">
    <attributes
        name="Age"/>
    <attributes
        name="Name"/>
  </classes>
  <classes name="Employee"
      persistent="true"
      super="//@classes.0">
    <attributes
        name="Project"/>
  </classes>
</uml:Package>
