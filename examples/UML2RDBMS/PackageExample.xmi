<?xml version="1.0" encoding="ASCII"?>
<echo.examples.uml2rdbms.UML:Package
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:echo.examples.uml2rdbms.UML="http://fatbit.uminho.pt/echo/examples/uml2rdbms/"
    xsi:schemaLocation="http://fatbit.uminho.pt/echo/examples/uml2rdbms/ UML.ecore"
    name="Company">
  <classes name="Person">
    <attributes
        name="Name"/>
    <attributes
        name="Age"/>
  </classes>
  <classes name="Employer"
      persistent="true"
      general="//@classes.0">
    <attributes
        name="Group"/>
  </classes>
  <classes name="Employee"
      persistent="true"
      general="//@classes.0">
    <attributes
        name="Project"/>
  </classes>
</echo.examples.uml2rdbms.UML:Package>
