<?xml version="1.0" encoding="ASCII"?>
<pt.uminho.di.haslab.uml:Package
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:pt.uminho.di.haslab.uml="pt.uminho.di.haslab.uml"
    xsi:schemaLocation="pt.uminho.di.haslab.uml UML.ecore"
    name="Company">
  <classes name="Person">
    <attributes
        name="Name"/>
    <attributes
        name="Age"/>
  </classes>
  <classes name="Employer"
      persistent="true"
      general="//@classes.0">
    <attributes
        name="Group"/>
  </classes>
  <classes name="Employee"
      persistent="true"
      general="//@classes.0">
    <attributes
        name="Project"/>
  </classes>
</pt.uminho.di.haslab.uml:Package>
