transformation NonTopEnforce (m1 : TwoLayer , m2 : TwoLayer) {

	top relation R {
  		n : String;
  		firstchild : TwoLayer::Child;
  		secondchild : TwoLayer::Child;
  		enforce domain m1 me1:Top {name = n, child = firstchild};
  		enforce domain m2 me2:Top {name = n, child = secondchild};
  		where { S(firstchild,secondchild); }
  	}
	
	relation S {
 		n : String;
  		enforce domain m1 me1:Child {name = n};
  		enforce domain m2 me2:Child {name = n};
  	}
  		
 }