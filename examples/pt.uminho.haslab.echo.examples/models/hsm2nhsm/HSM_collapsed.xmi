<?xml version="1.0" encoding="ASCII"?>
<hsm:StateMachine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:hsm="HSM"
    xsi:schemaLocation="HSM ../../metamodels/hsm2nhsm/HSM.ecore"
    name="machine">
  <states xsi:type="hsm:CompositeState"
      name="Active"/>
  <states name="Waiting"/>
  <transitions
      source="//@states.0"
      target="//@states.1"
      machine="/"/>
</hsm:StateMachine>
