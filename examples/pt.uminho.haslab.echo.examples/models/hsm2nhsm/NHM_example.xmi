<?xml version="1.0" encoding="ASCII"?>
<nhsm:StateMachine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:nhsm="NHSM"
    xsi:schemaLocation="NHSM /pt.uminho.haslab.echo.examples/metamodels/hsm2nhsm/NHSM.ecore"
    name="machine">
  <states name="Idle"/>
  <states name="Active"/>
  <transitions
      source="//@states.0"
      target="//@states.1"/>
  <transitions
      source="//@states.1"
      target="//@states.0"/>
</nhsm:StateMachine>
