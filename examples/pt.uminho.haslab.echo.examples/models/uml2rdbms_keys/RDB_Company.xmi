<?xml version="1.0" encoding="ASCII"?>
<rdbms:Scheme
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:rdbms="RDBMS"
    xsi:schemaLocation="RDBMS ../../metamodels/uml2rdbms_keys/RDBMS.ecore#RDBMS"
    name="Company">
  <tables name="Employer">
    <columns
        name="name"/>
  </tables>
  <tables name="Employee">
    <columns
        name="salary"/>
    <columns
        name="name"/>
    <columns
        name="department"/>
  </tables>
  <tables name="Department">
    <key column="//@tables.2/@columns.0"/>
    <columns
        name="location"/>
  </tables>
  <keys refersTo="//@tables.2/@key"
      column="//@tables.1/@columns.2"/>
</rdbms:Scheme>
