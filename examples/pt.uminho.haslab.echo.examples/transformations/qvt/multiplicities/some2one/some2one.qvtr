import A : '../../../../metamodels/multiplicities/A_attributes.ecore' :: AAttributes;
import B : '../../../../metamodels/multiplicities/B_base.ecore' :: BBase;

transformation a2b (as : A, bs : B) {

	top relation A2B { 
    	domain as ra:RootA {};
    	domain bs rb:RootB {};
    	where { ra.as->size() = rb.bs->size(); }    	
  	}
  	
}